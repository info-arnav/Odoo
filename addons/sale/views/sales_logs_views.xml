<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="view_action_orders_logs" model="ir.ui.view">
        <field name="name">orders.logs.list</field>
        <field name="model">orders.logs</field>
        <field name="arch" type="xml">
            <list string="Product List">
                <field name="order_id"/>
                <field name="updated" />
            </list>
        </field>
    </record>

    <record id="action_orders_logs" model="ir.actions.act_window">
        <field name="name">Logs</field>
        <field name="res_model">orders.logs</field>
        <field name="view_mode">list</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                See logs.
            </p>
        </field>
    </record>
</odoo>
